  "dependencies": {
    "baileys": "npm:baileys-mod",
    "events": "3.3.0",
    "glob": "11.0.3",
    "path": "0.12.7",
    "qr-image": "3.2.0",
    "util": "^0.12.5"
  },
  "devDependencies": {
    "@types/node": "22.13.8",
